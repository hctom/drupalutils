{
  "name": "hctom/drupalutils",
  "description": "Command line providing utilities for Drupal.",
  "authors": [
    {
      "name": "hctom (Thomas Keitel)",
      "email": "info@hctom.net",
      "homepage": "http://www.hctom.net/",
      "role": "Developer"
    }
  ],
  "require": {
    "symfony/console": "~2.3",
    "symfony/process": "~2.1",
    "symfony/filesystem": "~2.6",
    "psr/log": "~1.0",
    "symfony/class-loader": "~2.6",
    "monolog/monolog": "^1.13"
  },
  "bin": [
    "drupalutils"
  ],  
  "autoload": {
    "psr-4": {
      "hctom\\DrupalUtils\\": "src/drupal/",
      "hctom\\DrushWrapper\\": "src/drush/"
    }
  },
  "require-dev": {
    "drush/drush": "dev-master"
  }
}
